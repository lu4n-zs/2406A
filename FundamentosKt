    */    
}
//--------------------------------------------------------------------------------------------------------------------------------------------------------
fun main() {
    // Laços de repetição - loops
    // 
    // repeat - repita
    // while - enquanto 
    // do...whilw - faça... enquanto
    // for - para
    // 
    // repeat - repita 
    // vai repetir uma determinada ação uma determinada quantidade de vezes 
    // 
    // Estrutuda do Repeat
    // 
    // repeat(numero de vezes){
    // ação
    // }
    //
    /*
    repeat(8){
        println("isso é um teste, testando, testando, testando")
    }
    */
    
    /*
    //  Exercício -> Faça um programa com auxilio de uma variável contador,
    // no qual nela será apresentado 1 a cada repetição,
    // até que dê a quantidade de vezes decididas
    
    var contador = 0
    
    repeat(10){ 
        println(contador++)
    }
    */
    
    // Laço while - enquanto 
    // 
    // enqaunto uma condição for verdadeira, uma determinada ação sera sendo executada 
    // 
    // while(condição){
    // ação
    // }
    /*
    var contador = 0
    while(contador < 10){
        println(contador++)
    }
    */
    
    // Exercício -> Simulador de bateria de celular 
    // Faça um programa com while que mostra a bateria 100% diminuindo.
    // Quando ela chegar a 15%, ela deve mostrar uma mensagem pedindo para 
    // conectar o carregador, e quando chegar a 0%, deve mostrar a mensagem aparelho desligando.
    
    var contador = 0
    println("Bateria: 0%, desligando...")
    while(contador < 0){
        println(contador++)
    }
    println("Bateria: 15%, coloque para carregar")
    while(contador < 16){
        println(contador++)
    }
    
    
    
}
//----------------------------------------------------------------------------------------------------------------------------------------------------------
fun main() {
    
    
    //Exercício -> Simulador de Bateria de celular
    // faça um programa com while que mostre a bateria do celular 100% diminuindo. 
    // Quando ela chegar no 15, deve mostrar a mesnsagem pedindo para 
    // conectar o carrgador, e quando chegar a 0, deve mostrar a mesnsagem
    // aparelho desligando
    
    /*
    var bateria = 100 
    
    while(bateria >= 0){
        println("$bateria%")
        
        when(bateria){
            15 -> {println("Aparelho descarregando, conecte no carregador!")}
            0 -> {println("Aparelho Desligando!")}
        }
        bateria--
        
    }
    */
    // while -> ENQUANTO uma determinada condição for verdadeira, uma determinada ação será 
    // executada
    /*
    var contador = 0
    while(contador > 10){
        println(contador++)
    } 
    */
    // do..while 
    // 
    // uma determinada ação será executada, ENQUANTO uma determinada condição for
    // verdadeira
    
    //Estrutura: 
    //
    // do{
    // ação
    // }while(condição) 
    /*
    var contador = 0
    
    do{
        println(contador++)
    }while(contador > 10) 
    */ 
    
    // for - para...  
    // 
    // PARA cada item em um interválo ou colection, uma determinada ação será executada 
    // Estrutura:
    // 
    // for(vAuxiliar in inteválo ou colection){
    // ação
    // } 
    // 
    
    for(pneu in 1..4){
        println("$pneu")
    } 
    
   // Exercício -> Faça uma tabuada até o décimo multiplo de um número escolhido, 
   // utilizando o laço for 
   // em excessão as tabuadas do 0, 1 e 2
    
    
    
    
    
    
    
    
}
//----------------------------------------------------------------------------------------------------------------------------------------------------------------
fun main() {
    /*
    for(pneu in 1..4){
        println("$pneu")
    } 
    */
    
   // Exercício -> Faça uma tabuada até o décimo multiplo de um número escolhido, 
   // utilizando o laço for 
   // em excessão as tabuadas do 0, 1 e 2
   // 
   // Correção
   // 
   // 
   /*
   for(calc in 0..10){
       println(7 * calc)
   }
   */
   
   // Collections 
   // 
   // intervalos, listas, arrays, vetores etc 
   
   // Tipos de listas 
   // 
   /
   var list1 = listOf("tartaruga", 0, 0.1, true)
   
   println(list1) //exibir a lista
   println(list1.size) //retorna o tamanho da lista
   println(list1.lastIndex) //retornar o último index 
   println(list1[2]) // exibir o valor de um determinado index
   println(list1.contains("Tartaruga")) //compara se existe um determinado valor dentro da lista
   println(list1.indexOf(0)) //indica o index de um determinado valor 
   println(list1.lastIndexOf(0.1)) //indica o último de um determinado valor
   println(list1.first()) //o valor do primeiro index
   println(list1.last()) //o valor do último index
   println(list1.isEmpty()) //compara se a lista está vazia
   
   //<tags>
   var list2 = listOf<>("LeBron James", "Stephen Curry", "Michael Jordan", "Perna Longa", "Klay Thompson")
   // lista acima especifica o tipo de dados que vai ser guardado
   // 
   // obs. listOf() -> imutável
   // próxima aula... mutableListOf()
   
   
   
  
}
//------------------------------------------------------------------------------------------------------------------------------------
fun main() {
    
    // obs. listOf() -> imutável
   // próxima aula... mutableListOf()
   // mutableListOf() --> armazena qualquer tipo de valor conjunto 
   // mutableListOf<tipoDeValor> -> armazena valores especificos de seu tipo
   // 
   /*
   var comida = mutableListOf<String> ("strogonoff", "patê de frango", "lasanha", "feijoada")
   
   println(comida)
   
   // Propriedade
   // 
   comida.add("macarrão") //adiciona um valor lista
   println(comida) 
   comida.add(0, "cuscuz") //adiciona um valor a um index em específico na lista
   println(comida)
   comida.remove("patê de frango") //remove um valor em específico na lista
   println(comida)
   comida.removeAt(0) // remove o valor de um index em específico
   println(comida)
   comida.set(2, "pastel de carne seca") //subistitui um valor pelo seu index
   println(comida)
   comida.clear() //remove todos os valores da lista 
   println(comida)
   */
   
   //  Exercício -> a cada mês do ano, em uma turma de 5 alunos, entra um aluno novo
   //  .crie a lista oficial de alunos da turma 
   //  .cada mês adicione um novo aluno (em linhas diferentes)
   //  .e 2 mêses do ano 2 alunos transferidos da turma 
   //  .e em dois mêses do ano, 2 alunos foram substituidos em mêses por
   //  .outros dois alunos (dica: em mêses diferentes)
   //  
   //  Exiba a lista de alunos respectiva de cada mês do ano
   //  dica (interpretação de texto)
   //  
   var alunos = mutableListOf<String> ("Allan", "Bianca", "Clara", "Davi", "Eduardo")
   
   println("Janeiro")
   alunos.add("Felipe")
   println(alunos)
   
   println("Fevereiro")
   alunos.add("Gabriel")
   println(alunos)
   alunos.remove("Bianca")
   println(alunos)
   
   println("Março")
   println(alunos)
   alunos.add("Hariel")
   println(alunos)
   alunos.set(5, "Isabelly")
   println(alunos)
   
   
   
   
   
   
   
   
   
}
//---------------------------------------------------------------------------------------------------------------------------------------------------------------
fun main() {
     //  Exercício -> a cada mês do ano, em uma turma de 5 alunos, entra um aluno novo
   //  .crie a lista oficial de alunos da turma 
   //  .cada mês adicione um novo aluno (em linhas diferentes)
   //  .e 2 mêses do ano 2 alunos transferidos da turma 
   //  .e em dois mêses do ano, 2 alunos foram substituidos em mêses por
   //  .outros dois alunos (dica: em mêses diferentes)
   //  
   //  Exiba a lista de alunos respectiva de cada mês do ano
   //  dica (interpretação de texto)
